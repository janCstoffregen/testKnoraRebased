@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/>.
@prefix event: <http://www.knora.org/ontology/event#>.
@prefix human: <http://www.knora.org/ontology/human#>.
@prefix lang: <http://www.knora.org/ontology/language#>.
@prefix concept: <http://www.knora.org/ontology/concept#>.
@prefix infocar: <http://www.knora.org/ontology/information-carrier#>.
@prefix publish: <http://www.knora.org/ontology/publishing#>.
@prefix litera: <http://www.knora.org/ontology/literature#>.
@prefix knora-base: <http://www.knora.org/ontology/knora-base#>.
@prefix salsah-gui: <http://www.knora.org/ontology/salsah-gui#>.
@prefix text: <http://www.knora.org/ontology/text#>.

<http://www.knora.org/ontology/text>
	a owl:Ontology;
	dct:title "An ontology about text"@en;
	dct:description """Formal description of text in broadest sense, general subclasses, related classes, and properties."""@en;
	dct:creator "Hans Cools, software developer, NIE-INE, Basel University, Switzerland"@en;
	dct:contributor "Roberta Padlina, tool coordinator, NIE-INE, Basel University, Switzerland"@en;
	dct:publisher "Basel University, Switzerland"@en.
#	skos:note """Representation of an otherwise literal data element as object ('objectivation') enabling relating it to metadata"""@en.

#
# CLASSES
#

# text:Text

text:Expression
	a owl:Class;
	rdfs:label "text expression"@en, "Text-Expression"@de;
	rdfs:comment """Text as a human luinguistic expression."""@en;
	rdfs:subClassOf concept:Expression, lang:HumanNaturalLanguageExpression, [
		a owl:Restriction; owl:onProperty text:hasAuthor; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasAuthorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasRevisor; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasRevisorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasEditor; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasEditorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasGlossator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasGlossatorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasAbbreviator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasAbbreviatorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasCopyist; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasCopyistValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasRedactor; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasRedactorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasContent; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasEdition; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasEditionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasSameEditionAs; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasSameEditionAsValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasDiplomaticTranscription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasDiplomaticTranscriptionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty lang:expressedInNaturalLanguage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty lang:expressedInNaturalLanguageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty lang:expressedInDialect; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty lang:expressedInDialectValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty lang:hasTranslator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty lang:hasTranslatorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty lang:translatedInNaturalLanguage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty lang:translatedInNaturalLanguageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty infocar:isOnCarrier; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty infocar:isOnCarrierValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [	
		a owl:Restriction; owl:onProperty text:hasStructure; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasStructureValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasIncipit; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasExplicit; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasPageIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasPageNumberIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasColumnIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasLineNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasVersionIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isPartOfCycle; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasReferenceText; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasReferenceTextValue; owl:minCardinality "0"^^xsd:nonNegativeInteger] #, [
#		a owl:Restriction; owl:onProperty publish:hasPublicationNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger]
		.

text:Author
	a owl:Class;
	rdfs:label "author"@en, "Autor"@de;
	rdfs:comment """Role of a person being responsible for a text."""@en;
	skos:note """Often an author is also the creator of a text."""@en;
	rdfs:subClassOf human:Role.

text:Revisor
	a owl:Class;
	rdfs:label "revisor"@en, "Revisor"@de;
	rdfs:comment """Role of a person checking on certain aspects of a text."""@en;
	rdfs:subClassOf human:Role.

text:Editor
	a owl:Class;
	rdfs:label "text editor"@en, "Text-Redakteur"@de;
	rdfs:comment """Role of a person changing a text."""@en;
	rdfs:subClassOf human:Creator.

text:Derivative
	a owl:Class;
	rdfs:label "text derivative"@en, "abgeleitete Text"@de;
	rdfs:comment """Text expression derived from another text expression."""@en;
	rdfs:subClassOf text:Expression, human:CreationDerivative.

text:Edition # can be on >= 1 carrier
	a owl:Class;
	rdfs:label "text edition"@en, "Text-Edition"@de; # Lesetext subClassOf
	rdfs:comment """Changed form of a text expression as a result of editing."""@en;
	rdfs:subClassOf text:Derivative.

text:Redactor #* -> scholastic?
	a owl:Class;
	rdfs:label "redactor"@la, "redactor"@en, "Redactor"@de;
	rdfs:comment """Role of a person assembling (pieces of) texts to a new coherent text."""@en;
	rdfs:subClassOf human:Creator.

text:Glossator #* -> scholastic? Unlikely to be used in modern times?
	a owl:Class;
	rdfs:label "glossator"@la, "glossator"@en, "Glossator"@de;
	rdfs:comment """Role of a person adding texts or glosses to a text."""@en;
# 	substantial OR marginal texts e.g. in the margin or between lines
	rdfs:subClassOf human:Creator.

text:Abbreviator #* -> scholastic?
	a owl:Class;
	rdfs:label "abbreviator"@la, "abbreviator"@en, "Abkürzer"@de;
	rdfs:comment """Role of a person being responsible for a shortened version of a text."""@en;
	rdfs:subClassOf human:Creator.

text:Copyist #* -> scholastic? ~ handwriting
	a owl:Class;
	rdfs:label "scribes"@la, "copyist"@en, "Kopist"@de; # "scribe"@en, "Schreiber"@de;
	rdfs:comment """Role of a person copying text by writing."""@en;
	rdfs:subClassOf human:Creator.

text:Translator #* -> scholastic? ~ handwriting
	a owl:Class;
	rdfs:label "translator"@en, "Übersetzer"@de;
	rdfs:comment """Role of a person translating text by writing."""@en;
	rdfs:subClassOf lang:Translator, human:Creator.

text:WrittenText
	a owl:Class;
	rdfs:label "written text"@en, "geschriebener Text"@de;
	rdfs:comment """Text written by any method, e.g. manually, with an analogue or digital typewriter, or on a computer, abstracted from its carrier."""@en;
	rdfs:subClassOf lang:HumanNaturalLanguageExpression, [
		a owl:Restriction; owl:onProperty infocar:isWrittenWith; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isWrittenWithValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:HandwrittenText
	a owl:Class;
	rdfs:label "handwritten text"@en, "handgeschriebener Text"@de;
	rdfs:comment """Text written by hand."""@en;
	rdfs:subClassOf text:WrittenText, [
		a owl:Restriction; owl:onProperty infocar:isInManuscript; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isInManuscriptValue; owl:cardinality "1"^^xsd:nonNegativeInteger].

text:TypewrittenText
	a owl:Class;
	rdfs:label "typewritten text"@en, "maschinengeschriebener Text"@de;
	rdfs:comment """Text written with a typewriter."""@en;
	rdfs:subClassOf text:WrittenText, [
		a owl:Restriction; owl:onProperty infocar:isInTypescript; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isInTypescriptValue; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isWrittenWith; owl:someValuesFrom text:Typewriter].

text:Structure
	a owl:Class;
	rdfs:label "text structure"@en, "Textstruktur"@de;
	rdfs:comment """Human natural language expression as textual structure, e.g. a sentence, text line, verse, paragraph, strophe."""@en;
	skos:note """Making up a text:Expression, without necessarily being one itself."""@en;
	rdfs:subClassOf lang:HumanNaturalLanguageExpression.

# syntactical element rdfs:subClassOf text:Structure: one or more words that are grammatically linked

text:Sentence
	a owl:Class;
	rdfs:label "sentence"@en, "Satz"@de;
	rdfs:comment """Syntactical unit as one or more grammatically linked words, typically a subject and predicate."""@en;
	rdfs:subClassOf text:Structure.

# text:WrittenSentence: orthographic element: one or more words starting with a capital letter and ending in a full stop or period, question mark, or exclamation mark

text:Line
	a owl:Class;
	rdfs:label "text line"@en, "Textzeile"@de;
	rdfs:comment """Line of text."""@en;
	rdfs:subClassOf text:Structure, [
		a owl:Restriction; owl:onProperty text:isLineOf; owl:someValuesFrom text:Expression], [
		a owl:Restriction; owl:onProperty text:isLineOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isLineOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:Paragraph
	a owl:Class;
	rdfs:label "paragraph"@en, "Paragraf"@de;
	rdfs:comment """Group of text lines."""@en;
	rdfs:subClassOf text:Structure, [
		a owl:Restriction; owl:onProperty text:isParagraphOf; owl:someValuesFrom text:Expression], [
		a owl:Restriction; owl:onProperty text:isParagraphOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isParagraphOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:Prose
	a owl:Class;
	rdfs:label "prose"@en, "Prosa"@de;
	rdfs:comment """Text structure with a natural flow of speech or grammatical structure."""@en;
	rdfs:subClassOf text:Structure, [
		a owl:Restriction; owl:onProperty text:hasStructure; owl:someValuesFrom text:Sentence], [
		a owl:Restriction; owl:onProperty text:hasStructure; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasStructureValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:NoteProse #* = prose?
	a owl:Class;
	rdfs:label "note prose"@en, "Prosanotat"@de;
	rdfs:comment """Prose with note structure."""@en;
	rdfs:subClassOf text:Structure.

text:Verse
	a owl:Class;
	rdfs:label "verse"@en, "Vers"@de;
	rdfs:comment """Text line with metrical structure."""@en;
	rdfs:subClassOf text:Line, [
		a owl:Restriction; owl:onProperty text:isVerseOf; owl:someValuesFrom litera:Poem], [ # OR litera:VerseNovel
		a owl:Restriction; owl:onProperty text:isVerseOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isVerseOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:Strophe #* OR stanza OR verse - Vers
	a owl:Class;
	rdfs:label "strophe"@en, "Strophe"@de;
	rdfs:comment """Paragraph of verses as part of a poem."""@en;
	skos:note """The lyriks of a song can be structured as a poem with strophes."""@en;
	rdfs:subClassOf text:Paragraph, [
#		a owl:Restriction; owl:onProperty concept:isStropheOf; owl:someValuesFrom litera:Poem], [
		a owl:Restriction; owl:onProperty concept:isStropheOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty concept:isStropheOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:FreeVerse
	a owl:Class;
	rdfs:label "free verse"@en, "reimloser Vers"@de;
	rdfs:comment """Verse without rhyming."""@en;
	rdfs:subClassOf text:Verse.

text:RhymingVerse
	a owl:Class;
	rdfs:label "rhyming verse"@en, "gereimter Vers"@de;
	rdfs:comment """Verse with rhyming."""@en;
	rdfs:subClassOf text:Verse.

# Prosaic Verse, Prosagedicht #*

text:RhythmicProse #*
	a owl:Class;
	rdfs:label "rhythmic Prose"@en, "rhythmische Prosa"@de;
	rdfs:comment """Prose ... ."""@en;
	rdfs:subClassOf text:Structure.

text:Note #*
	a owl:Class;
	rdfs:label "note"@en, "Notiz"@de;
	rdfs:comment """Text with note structure."""@en;
	rdfs:subClassOf text:Structure.

text:LetterStructure #*
	a owl:Class;
	rdfs:label "letter structure"@en, "Brief-Struktur"@de;
	rdfs:comment """Text with letter structure."""@en;
	rdfs:subClassOf text:Structure.

text:Draft
	a owl:Class;
	rdfs:label "draft"@en, "Entwurf"@de;
	rdfs:comment """Developmental text expression."""@en;
	rdfs:subClassOf human:CreationVersion.

text:DiaryEntry
	a owl:Class;
	rdfs:label "diary entry"@en, "Tagebucheintrag"@de;
	rdfs:comment """Periodical, usually daily description of a personal experience or observation written in a diary."""@en;
	rdfs:subClassOf text:Expression, [
		a owl:Restriction; owl:onProperty infocar:isInDiary; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isInDiaryValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:hasDiaryEnteringDate; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:LetterExpression
	a owl:Class;
	rdfs:label "letter expression"@en, "Brief-Expression"@de;
	rdfs:comment """Text expression with a letter structure."""@en;
	rdfs:subClassOf text:Expression, [
		a owl:Restriction; owl:onProperty infocar:isInLetter; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty infocar:isInLetterValue; owl:cardinality "1"^^xsd:nonNegativeInteger].

text:Commentary
	a owl:Class;
	rdfs:label "commentary"@en, "Kommentar"@de;
	rdfs:comment """Text expression in the form of a series of comments as analysis, interpretation or explanation of something, especially another textual expression."""@en;
	rdfs:subClassOf text:Expression.

text:DiplomaticTranscription
	a owl:Class;
	rdfs:label "diplomatic transcription"@en, "diplomatische Umschrift"@de;
	rdfs:comment """Transcription of a text with diacritical signs indicating visual textual properties to describe the textual state."""@en;
	rdfs:subClassOf text:Derivative, [
		a owl:Restriction; owl:onProperty text:isDiplomaticTranscriptionOfTextOnPage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isDiplomaticTranscriptionOfTextOnPageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger]#, [
#		a owl:Restriction; owl:onProperty infocar:isOnCarrier; owl:cardinality "1"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty infocar:isOnCarrierValue; owl:cardinality "1"^^xsd:nonNegativeInteger]
		.

text:CriticalEdition
	a owl:Class;
	rdfs:label "critical text edition"@en, "kritische Text-Edition"@de;
	rdfs:comment """."""@en; #* scientist adding interpretation of the text expression
	rdfs:subClassOf text:Edition.

text:Development
	a owl:Class;
	rdfs:label "text development"@en, "Text-Entwicklung"@de;
	rdfs:comment """Action wherein a person(s) creates text resulting in different manifestation, e.g. from notebook to manuscript to typescript to published print."""@en;
	rdfs:subClassOf human:Creating, [
		a owl:Restriction; owl:onProperty event:hasStage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty event:hasStageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

#
# PROPERTIES
#

text:hasText
	a owl:ObjectProperty;
	rdfs:label "has text"@en, "hat Text"@de;
	rdfs:comment """Relating a resource to a text (as object) as a writable coherent set of signs representing semantics in a language."""@en;
	rdfs:subPropertyOf knora-base:hasValue;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasName
	a owl:ObjectProperty;
	rdfs:label "has name"@en, "hat Name"@de;
	rdfs:comment """Relating a resource to a name (as object) as a textual identifier of the resource."""@en;
	rdfs:domain rdfs:Resource;
	rdfs:range rdfs:Literal;
	rdfs:subPropertyOf text:hasText; # rdfs:label
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasPreferredName
	a owl:ObjectProperty;
	rdfs:label "has preferred name"@en, "hat bevorzugter Name"@de;
	rdfs:comment """Relating a resource to a preferred name (as object)."""@en;
	rdfs:subPropertyOf text:hasName;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasAlias
	a owl:ObjectProperty;
	rdfs:label "has alias"@en, "hat Alias"@de;
	rdfs:comment """Relating a resource to an alias (as object) as an alternative name of the resource."""@en;
	rdfs:subPropertyOf text:hasName;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasIdentifier
	a owl:ObjectProperty;
	rdfs:label "has identifier"@en, "hat Kennzeichen"@de;
	rdfs:comment """Relating a resource to an identifier (as object) as a unique textual reference to the resource within a given context, and conforming to a formal identification system."""@en;
	rdfs:subPropertyOf text:hasText; # cidoc:E42_Identifier, cf. dcterms:identifier
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasTitle
	a owl:ObjectProperty;
	rdfs:label "has title"@en, "hat Titel"@de;
	rdfs:comment """Relating a resource to a title (as object) as a textual identifier of the resource."""@en;
	rdfs:subPropertyOf text:hasText;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasComment
	a owl:ObjectProperty;
	rdfs:label "has comment"@en, "hat Kommentar"@de;
	rdfs:comment """Relating a resource to a comment (as object) as a textual statement about the resource."""@en;
	rdfs:subPropertyOf text:hasText;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasDescription
	a owl:ObjectProperty;
	rdfs:label "has description"@en, "hat Beschreibung"@de;
	rdfs:comment """Relating a resource to a description (as object) as a textual statement about features of the resource."""@en;
	rdfs:subPropertyOf text:hasText;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasDefinition
	a owl:ObjectProperty;
	rdfs:label "has definition"@en, "hat Definition"@de;
	rdfs:comment """Relating a resource to a definition (as object) as a textual statement about the essential features of the resource."""@en;
	rdfs:subPropertyOf text:hasDescription;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasDetailDescription
	a owl:ObjectProperty;
	rdfs:label "has detail description"@en, "hat Detail-Beschreibung"@de;
	rdfs:comment """Relating a resource to a description (as object) of its detail(s). E.g. in the case of a text a detail can be a passage variant or an emendation"""@en;
	rdfs:subPropertyOf text:hasDescription;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
